<h2>Add Mountain</h2>
<form class="" action="/mountains" method="post">

  <label>
    Name:
    <input type="text" name="name">
  </label>
  <br>

  <label>
    Location:
    <input type="text" name="location">
  </label>

  <br>
  <label>Height:
    <input type="text" name="height">
  </label>
  <br>

  <label>
    Scalable:
    <input type="text" name="scalable">
  </label>
  <br>

  <label>First Ascent:
    <input type="text" name="first_ascent">
  </label>
  <br>

  <label>Image:
    <input type="text" name="image_url">
  </label>
  <br>

  <input type="submit" name="" value="Create Mountain">
</form>
